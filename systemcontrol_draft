import alsaaudio
import sys

def controlvolume(keyboard):
    if keyboard== 'h':
        m= alsaaudio.Mixer()
        m.setvolume(50)
        vol= m.getvolume()
    elif keyboard== 'j':
        m= alsaaudio.Mixer()
        m.setvolume(+5)
        vol= m.getvolume()
    elif keyboard== 'g':
        m= alsaaudio.Mixer()
        m.setvolume(+5)
        vol= m.getvolume()
        
def main(argv):
    controlvolume(argv)
    

if __name__=="__main__":
    main(sys.argv[1:2])
